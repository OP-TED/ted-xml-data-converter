<?xml version="1.0" encoding="UTF-8"?>
<mappings>

<form-types>
	<description>Mapping for eForms schema documents root element name and namespace</description>
	<mapping>
		<abbreviation>PIN</abbreviation>
		<element-name>PriorInformationNotice</element-name>
		<xmlns>urn:oasis:names:specification:ubl:schema:xsd:PriorInformationNotice-2</xmlns>
	</mapping>
	<mapping>
		<abbreviation>CN</abbreviation>
		<element-name>ContractNotice</element-name>
		<xmlns>urn:oasis:names:specification:ubl:schema:xsd:ContractNotice-2</xmlns>
	</mapping>
	<mapping>
		<abbreviation>CAN</abbreviation>
		<element-name>ContractAwardNotice</element-name>
		<xmlns>urn:oasis:names:specification:ubl:schema:xsd:ContractAwardNotice-2</xmlns>
	</mapping>
	<mapping>
		<abbreviation>BRIN</abbreviation>
		<element-name></element-name>
		<xmlns></xmlns>
	</mapping>
</form-types>

<ca-types>
	<description>Mapping for Buyer Legal Type (BT-11)</description>
	<ted-context>/TED_EXPORT/FORM_SECTION/*/CONTRACTING_BODY/CA_TYPE/@VALUE</ted-context>
	<eforms-context>/*/cac:ContractingParty/cac:ContractingPartyType/cbc:PartyType</eforms-context>
	<eforms-codelist>buyer-legal-type</eforms-codelist>
	<mapping>
		<ted-value>BODY_PUBLIC</ted-value>
		<eforms-value>body-pl</eforms-value>
		<label>Body governed by public law</label>
	</mapping>
	<mapping>
		<ted-value>EU_INSTITUTION</ted-value>
		<eforms-value>eu-ins-bod-ag</eforms-value>
		<label>EU institution, body or agency</label>
	</mapping>
	<mapping>
		<ted-value>MINISTRY</ted-value>
		<eforms-value>cga</eforms-value>
		<label>Central government authority</label>
	</mapping>
	<mapping>
		<ted-value>NATIONAL_AGENCY</ted-value>
		<eforms-value>cga</eforms-value>
		<label>Central government authority</label>
	</mapping>
	<mapping>
		<ted-value>REGIONAL_AGENCY</ted-value>
		<eforms-value>body-pl-ra</eforms-value>
		<label>Body governed by public law, controlled by a regional authority</label>
	</mapping>
	<mapping>
		<ted-value>REGIONAL_AUTHORITY</ted-value>
		<eforms-value>ra</eforms-value>
		<label>Regional authority</label>
	</mapping>
</ca-types>

<procedure-types>
	<description>Mapping for Procedure Type (BT-105)</description>
	<ted-context>/TED_EXPORT/FORM_SECTION/*/PROCEDURE/(PT_OPEN|PT_RESTRICTED|PT_COMPETITIVE_NEGOTIATION|PT_COMPETITIVE_DIALOGUE|PT_INNOVATION_PARTNERSHIP)/fn:local-name()</ted-context>
	<eforms-context>/*/cac:TenderingProcess/cbc:ProcedureCode</eforms-context>
	<eforms-codelist>procurement-procedure-type</eforms-codelist>
	<mapping>
		<ted-value>PT_OPEN</ted-value>
		<eforms-value>open</eforms-value>
		<label>Open</label>
	</mapping>
	<mapping>
		<ted-value>PT_RESTRICTED</ted-value>
		<eforms-value>restricted</eforms-value>
		<label>Restricted</label>
	</mapping>
	<mapping>
		<ted-value>PT_COMPETITIVE_NEGOTIATION</ted-value>
		<eforms-value>neg-w-call</eforms-value>
		<label>Negotiated with prior publication of a call for competition / competitive with negotiation</label>
	</mapping>
	<mapping>
		<ted-value>PT_COMPETITIVE_DIALOGUE</ted-value>
		<eforms-value>comp-dial</eforms-value>
		<label>Competitive dialogue</label>
	</mapping>
	<mapping>
		<ted-value>PT_INNOVATION_PARTNERSHIP</ted-value>
		<eforms-value>innovation</eforms-value>
		<label>Innovation partnership</label>
	</mapping>
	<mapping>
		<ted-value>PT_INVOLVING_NEGOTIATION</ted-value>
		<eforms-value>neg-wo-call</eforms-value>
		<label>Negotiated without prior call for competition</label>
	</mapping>
	<mapping>
		<ted-value>PT_NEGOTIATED_WITH_PRIOR_CALL</ted-value>
		<eforms-value>neg-w-call</eforms-value>
		<label>Negotiated with prior publication of a call for competition / competitive with negotiation</label>
	</mapping>
</procedure-types>

<contract-nature-types>
	<description>Mapping for Main and Additional Nature (BT-23, BT-531)</description>
	<ted-context>/TED_EXPORT/FORM_SECTION/*/OBJECT_CONTRACT/TYPE_CONTRACT/@CTYPE</ted-context>
	<eforms-context>/*/cac:ProcurementProject/cbc:ProcurementTypeCode</eforms-context>
	<eforms-codelist>contract-nature</eforms-codelist>
	<mapping><ted-value>SERVICES</ted-value><eforms-value>services</eforms-value></mapping>
	<mapping><ted-value>SUPPLIES</ted-value><eforms-value>supplies</eforms-value></mapping>
	<mapping><ted-value>WORKS</ted-value><eforms-value>works</eforms-value></mapping>
</contract-nature-types>

